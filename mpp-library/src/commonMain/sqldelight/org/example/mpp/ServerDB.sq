CREATE TABLE Servers (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  title TEXT NOT NULL,
  url TEXT NOT NULL,
  token TEXT NOT NULL
);

selectAll:
SELECT *
FROM Servers;
--JOIN Servers.server_id

getServer:
SELECT *
FROM Servers WHERE id = ?;

insertServer:
INSERT INTO Servers(title, url, token)
VALUES (?, ?, ?);

updateServer:
UPDATE Servers
SET title = ?, url = ?, token = ? WHERE id = ?;

deleteServer:
DELETE FROM Servers WHERE id = ?;


